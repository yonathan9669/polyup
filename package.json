{
  "name": "polyup",
  "version": "0.1.2",
  "description": "Helps you upgrade from Polymer 0.5 to 1.0!",
  "repository": "PolymerLabs/polyup",
  "main": "lib/upgrade_html.js",
  "bin": "lib/node_main.js",
  "scripts": {
    "test": "node_modules/.bin/jshint --verbose *.js build/*.js lib/*.js test/*.js && node_modules/.bin/mocha",
    "test:watch": "watch 'npm test' ./lib ./test ./build",
    "browserify": "mkdir -p ./dist/ && browserify ./lib/browser_main.js -o ./dist/browser_package.js",
    "serve": "npm run browserify && npm run deps && node serve.js",
    "deps": "npm install && bower install && cd test/bower_0.5 && bower install && cd ../bower_1.0 && bower install"
  },
  "keywords": [
    "polymer"
  ],
  "author": "The Polymer Project Authors",
  "license": "BSD-3-Clause",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "colors": "^1.1.2",
    "es6-collections": "^0.5.1",
    "escodegen": "^1.6.1",
    "espree": "^2.0.3",
    "estree-walker": "rictic/estree-walker#distrib",
    "lodash": "^3.9.3",
    "nomnom": "^1.8.1",
    "string.prototype.endswith": "^0.2.0",
    "vulcanize": "^1.9.2",
    "whacko": "^0.18.1"
  },
  "devDependencies": {
    "browserify": "^10.2.4",
    "jshint": "^2.8.0",
    "mime": "^1.3.4",
    "mocha": "^2.2.5",
    "node-static": "^0.7.6",
    "node-static-alias": "^0.1.3",
    "through2": "^2.0.0",
    "watch": "^0.16.0"
  },
  "browser": {
    "fs": "./build/fake-fs.js"
  },
  "bugs": {
    "url": "https://github.com/PolymerLabs/polyup/issues"
  }
}
